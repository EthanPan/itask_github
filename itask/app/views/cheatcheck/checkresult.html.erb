<html>
<h1> 检测结果 </h1>
<table class="table table-hover table-striped">
	  <thead>  
		<tr>
			<th> 学生姓名</th>
			<th> 相似度 </th> 
		</tr>
	  </thead>                       
	<tbody>
		<% @res.each do |key,value| %>
			<tr>
				<td><%= key %></td>
				<td><%= sprintf("%0.02f%" , value ) %></td>
			</tr>
		<% end %>
	</tbody>
</table>
</html>