<table  class="table table-hover table-striped">                        
	<thead>
                <tr>
                  <th>学生</th>
                  <th>上交时间</th>
                  <th>附件名称</th>
                  <th>操作</th> 

                </tr>
  </thead> 
  <tbody> 
	<% studentCourseAssignments.each do |sta| %>     
	<tr>  
		<td><% if !sta.blank? %>
			<% if !sta.user.blank?%>
			    <%= sta.user.name %>
			    <%end%>
		   <%end%>
		</td> 
		<td><%=sta.created_at%></td> 
		<th><% if !sta.attachments.blank? %>
			    <% sta.attachments.each do |a| %>
			    	<%= a.user_upload_file_name %>
			    <%end%>
		   <%end%></th>
		<td></td> 
	</tr> 
	<%end%>   
	</tbody>
</table>