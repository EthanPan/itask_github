<html>
<div class="container-fluid">
  <div class="row-fluid">
    <div class="span9 hero-unit">
     <h1> 课程列表 </h1>
<table class="table table-hover table-striped">
	  <thead>  
	<tr>
		<th> 学年 学期 </th> 
		<th> 课程序号 </th> 
		<th> 课程名称 </th>
		<th> 任课教师 </th>
	</tr>
	  </thead>                       
	<tbody>
	<% @course_years.each do |c| %>
	<tr>
		<td><%= c.year %> <%= c.semester %></td>
		<td><%= c.course.course_num %></td>
		<td><%= c.course.name %></td>
		<td><%= c.course.user.name %></td>
		<td><%= link_to "View" , course_year_path(c) %></td>
<!-- 		<% if can? :destroy, c %>
  			<td><%= link_to "delete", course_year_path(c),:method=>"delete" %></td>
		<% end %> -->
	</tr>
	<% end %>  
	</tbody>
</table>
    </div>
    <div class="span3 hero-unit">
      <!--Body content-->
    </div>
  </div>
</div>


</html>
	